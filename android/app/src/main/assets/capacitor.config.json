{
	"appId": "io.ionic.starter",
	"appName": "expense-record",
	"webDir": "www",
	"bundledWebRuntime": false,
	"plugins": {
		"CapacitorFirebaseAuth": {
			"providers": [
				"google.com",
				"twitter.com",
				"facebook.com",
				"phone"
			],
			"languageCode": "en",
			"nativeAuth": false,
			"properties": {
				"google": {
					"hostedDomain": "my-custom-domain.com"
				}
			},
			"permissions": {
				"google": [
					"profile",
					"https://www.googleapis.com/auth/drive"
				]
			}
		}
	}
}
